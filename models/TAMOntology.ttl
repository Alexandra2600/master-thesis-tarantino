#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/alexandra.tarantino/tam#Activity
:Activity rdf:type owl:Class ;
          rdfs:subClassOf :TAMThing ;
          rdfs:comment "A specific action performed by an agent or planned for the future, which may be part of a project or a routine."@en ;
          rdfs:label "Activity"@en .


###  http://www.semanticweb.org/alexandra.tarantino/tam#Agent
:Agent rdf:type owl:Class ;
       rdfs:subClassOf :TAMThing ;
       rdfs:comment "An individual or entity involved in activities and appointments, which can be the main user or other participants, and may also work on a project."@en ;
       rdfs:label "Agent"@en ;
       owl:equivalentClass "http://xmlns.com/foaf/spec/Agent" .

###  http://www.semanticweb.org/alexandra.tarantino/tam#Person
:Person rdf:type owl:Class ;
        rdfs:subClassOf :Agent ;
        rdfs:comment "A human being, which can be the main user or other participants."@en ;
        rdfs:label "Person"@en ;
        owl:equivalentClass "http://xmlns.com/foaf/spec/Person" .


###  http://www.semanticweb.org/alexandra.tarantino/tam#Event
:Event rdf:type owl:Class ;
       rdfs:subClassOf :TAMThing ;
       rdfs:comment "A specific occurrence or happening at a particular time and place, involving one or more agent"@en ;
       rdfs:label "Event"@en .


###  http://www.semanticweb.org/alexandra.tarantino/tam#Place
:Place rdf:type owl:Class ;
       rdfs:subClassOf :TAMThing ;
       rdfs:comment "A specific location or area where activities and events take place."@en ;
       rdfs:label "Place"@en .


###  http://www.semanticweb.org/alexandra.tarantino/tam#Project
:Project rdf:type owl:Class ;
         rdfs:subClassOf :TAMThing ;
         rdfs:comment "A set of activities and tasks planned and organized to achieve a specific goal or objective."@en ;
         rdfs:label "Project"@en .

###  http://www.semanticweb.org/alexandra.tarantino/tam#TAMThing
:TAMThing rdf:type owl:Class ;
              rdfs:comment "The superclass of all classes in the TAM ontology."@en ;
              rdfs:label "TAMThing"@en .


