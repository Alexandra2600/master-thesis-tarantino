#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/alexandra.tarantino/tam#involvedIn
:involvedIn rdf:type owl:ObjectProperty ;
           rdfs:domain :Agent ;
           rdfs:range :Activity, 
                        :Event ;
           rdfs:comment "An agent is involved in an activity or event."@en ;
           rdfs:label "involvedIn"@en .

###  http://www.semanticweb.org/alexandra.tarantino/tam#occursAt
:occursAt rdf:type owl:ObjectProperty ;
          rdfs:domain :Activity, 
                       :Event ;
          rdfs:range :Place ;
          rdfs:comment "An activity or event takes place at a specific location."@en ;
          rdfs:label "occursAt"@en .

###  http://www.semanticweb.org/alexandra.tarantino/tam#worksOn
:worksOn rdf:type owl:ObjectProperty ;
         rdfs:domain :Agent ;
         rdfs:range :Project ;
         rdfs:comment "An agent is involved in working on a project."@en ;
         rdfs:label "worksOn"@en .

###  http://www.semanticweb.org/alexandra.tarantino/tam#projectActivity
:projectActivity rdf:type owl:ObjectProperty ;
                 rdfs:domain :Project ;
                 rdfs:range :Activity ;
                 rdfs:comment "A project includes one or more activities."@en ;
                 rdfs:label "projectActivity"@en .


