###  http://www.semanticweb.org/alexandra.tarantino/tam#name
:name rdf:type owl:DatatypeProperty ;
      rdfs:domain :TAMThing ;
      rdfs:range xsd:string ;
      rdfs:comment "The name of TAM thing"@en ;
      rdfs:label "name"@en .

###  http://www.semanticweb.org/alexandra.tarantino/tam#description
:description rdf:type owl:DatatypeProperty ;
             rdfs:domain :TAMThing ;
             rdfs:range xsd:string ;
             rdfs:comment "A description of the TAM thing"@en ;
             rdfs:label "description"@en .

###  http://www.semanticweb.org/alexandra.tarantino/tam#statusActivity
:statusActivity rdf:type owl:DatatypeProperty ;
                rdfs:domain :Activity ;
                rdfs:range xsd:string ;
                rdfs:comment "The status of an activity, which can take only the following values: 'planned' or 'completed'"@en ;
                rdfs:label "statusActivity"@en .

###  http://www.semanticweb.org/alexandra.tarantino/tam#priority
:priority rdf:type owl:DatatypeProperty ;
          rdfs:domain :Activity, 
                       :Event, 
                       :Project ;
          rdfs:range xsd:string ;
          rdfs:comment "The priority of an activity, event or project that can take only the following values: 'low', 'medium', 'high' and 'important'"@en ;
          rdfs:label "priority"@en .

###  http://www.semanticweb.org/alexandra.tarantino/tam#startDateTime
:startDateTime rdf:type owl:DatatypeProperty ;
           rdfs:domain :Activity, 
                        :Event, 
                        :Project ;
           rdfs:range xsd:dateTime ;
           rdfs:comment "The start date of an activity, event or project"@en ;
           rdfs:label "startDateTime"@en .

###  http://www.semanticweb.org/alexandra.tarantino/tam#endDateTime
:endDateTime rdf:type owl:DatatypeProperty ;
         rdfs:domain :Activity, 
                      :Event, 
                      :Project ;
         rdfs:range xsd:dateTime ;
         rdfs:comment "The end date of an activity, event or project"@en ;
         rdfs:label "endDateTime"@en .

